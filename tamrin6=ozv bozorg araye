#include<iostream>
using namespace std;
int tedad_zoj(int* numbers,int s,int max)
{
    if(s==-1)
        return max;
    if(numbers[s]>max)
        max=numbers[s];
    return tedad_zoj(numbers,s-1,max);
}
int main()
{
    int n;cin>>n;
    int numbers[n];
    for(int i=0;i<n;i++)
    {
        cin>>numbers[i];
    }
    cout<<tedad_zoj(numbers,n-1,0);
    return 0;
}
