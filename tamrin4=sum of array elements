//recursive of the sum of array elements function
#include<iostream>
using namespace std;
int sum(int* numbers,int i,int n)
{
    if(i==n-1)
        return numbers[n-1];
    return numbers[i]+sum(numbers,i+1,n);
}
int main()
{
    int n;cin>>n;
    int numbers[n];
    for(int i=0;i<n;i++)
    {
        cin>>numbers[i];
    }
    cout<<sum(numbers,0,n);
    return 0;
}
